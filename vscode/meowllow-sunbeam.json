{
  "name": "Meowllow Sunbeam",
  "type": "light",
  "colors": {
    "editor.background": "#FDFBF8",
    "editor.foreground": "#423F3A",
    "editorCursor.foreground": "#7E9CD6",
    "editorLineNumber.foreground": "#8E8982",
    "editorLineNumber.activeForeground": "#7E9CD6",
    "editor.selectionBackground": "#7E9CD626",
    "editor.selectionHighlightBackground": "#7E9CD61F",
    "editor.findMatchBackground": "#E6B45038",
    "editor.findMatchHighlightBackground": "#7E9CD629",
    "editor.wordHighlightBackground": "#5FA8721F",
    "editor.wordHighlightStrongBackground": "#E88E861F",
    "editorBracketMatch.background": "#7E9CD61F",
    "editorBracketMatch.border": "#7E9CD6",
    "editorGutter.modifiedBackground": "#5FA872",
    "editorGutter.addedBackground": "#5FA872",
    "editorGutter.deletedBackground": "#C94B4B",
    "activityBar.background": "#F8F4F0",
    "activityBar.foreground": "#6D6863",
    "activityBarBadge.background": "#7E9CD6",
    "activityBarBadge.foreground": "#FDFBF8",
    "sideBar.background": "#F9F6F2",
    "sideBar.foreground": "#6D6863",
    "statusBar.background": "#F8F4F0",
    "titleBar.activeBackground": "#F8F4F0",
    "titleBar.activeForeground": "#423F3A",
    "titleBar.inactiveBackground": "#F9F6F2",
    "titleBar.inactiveForeground": "#8E8982",
    "tab.activeBackground": "#FDFBF8",
    "tab.inactiveBackground": "#F9F6F2",
    "tab.activeForeground": "#423F3A",
    "tab.inactiveForeground": "#8E8982",
    "tab.border": "#F9F6F2",
    "button.background": "#7E9CD624",
    "button.foreground": "#423F3A",
    "badge.background": "#7E9CD6",
    "badge.foreground": "#FDFBF8",
    "input.background": "#F2ECE4",
    "input.foreground": "#423F3A",
    "editorError.foreground": "#C94B4B",
    "editorWarning.foreground": "#C98939",
    "editorInfo.foreground": "#5279C6",
    "editorHint.foreground": "#81B580"
  },
  "semanticTokenColors": {
    "variable": "#423F3A",
    "variable.readonly": "#E6B450",
    "parameter": "#5EC2B8",
    "property": "#5EC2B8",
    "function": "#AB91E4",
    "method": "#AB91E4",
    "enumMember": "#E6B450",
    "class": "#5FA872",
    "type": "#5FA872",
    "interface": "#5FA872",
    "namespace": "#7E9CD6",
    "keyword": "#7E9CD6",
    "comment": {
      "foreground": "#B0A9A3",
      "fontStyle": "italic"
    },
    "string": "#E88E86",
    "number": "#E59BCF",
    "regexp": "#5EC2B8",
    "operator": "#8E8982"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#B0A9A3",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#7E9CD6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#7E9CD6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol",
        "string.quoted.template"
      ],
      "settings": {
        "foreground": "#E88E86"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#E59BCF"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "meta.function-call.identifier",
        "variable.function"
      ],
      "settings": {
        "foreground": "#AB91E4",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.method",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#AB91E4"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.class",
        "support.type",
        "storage.type.class",
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#5FA872"
      }
    },
    {
      "scope": [
        "constant",
        "constant.other",
        "variable.other.constant",
        "support.constant"
      ],
      "settings": {
        "foreground": "#E6B450"
      }
    },
    {
      "scope": [
        "variable",
        "identifier",
        "variable.parameter",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#423F3A"
      }
    },
    {
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name",
        "entity.name.tag.xml",
        "punctuation.definition.tag.xml",
        "meta.structure.dictionary.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#5EC2B8"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.attribute"
      ],
      "settings": {
        "foreground": "#5EC2B8"
      }
    },
    {
      "scope": [
        "support.type.property-name.css",
        "support.constant.property-value.css",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#7E9CD6"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "punctuation",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#8E8982"
      }
    },
    {
      "scope": [
        "string.regexp",
        "constant.other.regex"
      ],
      "settings": {
        "foreground": "#5EC2B8"
      }
    },
    {
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#AB91E4"
      }
    }
  ],
  "semanticHighlighting": true,
  "publisher": "meowllow",
  "description": "A soft, mellow, and soothing color theme inspired by creamy pastels and cozy coding sessions."
}
