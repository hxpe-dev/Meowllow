{
  "name": "Meowllow Slumber",
  "type": "dark",
  "colors": {
    "editor.background": "#2A272E",
    "editor.foreground": "#F6EDE6",
    "editorCursor.foreground": "#9FBCE8",
    "editorLineNumber.foreground": "#AFA9B8",
    "editorLineNumber.activeForeground": "#9FBCE8",
    "editor.selectionBackground": "#9FBCE833",
    "editor.selectionHighlightBackground": "#9FBCE81F",
    "editor.findMatchBackground": "#F9E2AF38",
    "editor.findMatchHighlightBackground": "#9FBCE829",
    "editor.wordHighlightBackground": "#8FCF981F",
    "editor.wordHighlightStrongBackground": "#EE99A01F",
    "editorBracketMatch.background": "#9FBCE81F",
    "editorBracketMatch.border": "#9FBCE8",
    "editorGutter.modifiedBackground": "#8FCF98",
    "editorGutter.addedBackground": "#8FCF98",
    "editorGutter.deletedBackground": "#E46A6A",
    "activityBar.background": "#1F1D25",
    "activityBar.foreground": "#D8D0C8",
    "activityBarBadge.background": "#9FBCE8",
    "activityBarBadge.foreground": "#2A272E",
    "sideBar.background": "#242128",
    "sideBar.foreground": "#D8D0C8",
    "statusBar.background": "#1F1D25",
    "titleBar.activeBackground": "#1F1D25",
    "titleBar.activeForeground": "#F6EDE6",
    "titleBar.inactiveBackground": "#242128",
    "titleBar.inactiveForeground": "#AFA9B8",
    "tab.activeBackground": "#2A272E",
    "tab.inactiveBackground": "#242128",
    "tab.activeForeground": "#F6EDE6",
    "tab.inactiveForeground": "#AFA9B8",
    "tab.border": "#242128",
    "button.background": "#9FBCE824",
    "button.foreground": "#F6EDE6",
    "badge.background": "#9FBCE8",
    "badge.foreground": "#2A272E",
    "input.background": "#4A4553",
    "input.foreground": "#F6EDE6",
    "editorError.foreground": "#E46A6A",
    "editorWarning.foreground": "#E5B76E",
    "editorInfo.foreground": "#7FA6F7",
    "editorHint.foreground": "#A6D1A1"
  },
  "semanticTokenColors": {
    "variable": "#F6EDE6",
    "variable.readonly": "#F9E2AF",
    "parameter": "#99E1C9",
    "property": "#99E1C9",
    "function": "#C7B0F0",
    "method": "#C7B0F0",
    "enumMember": "#F9E2AF",
    "class": "#8FCF98",
    "type": "#8FCF98",
    "interface": "#8FCF98",
    "namespace": "#9FBCE8",
    "keyword": "#9FBCE8",
    "comment": {
      "foreground": "#6E6A86",
      "fontStyle": "italic"
    },
    "string": "#EE99A0",
    "number": "#F5BDE6",
    "regexp": "#99E1C9",
    "operator": "#AFA9B8"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#6E6A86",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#9FBCE8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#9FBCE8",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol",
        "string.quoted.template"
      ],
      "settings": {
        "foreground": "#EE99A0"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#F5BDE6"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "meta.function-call.identifier",
        "variable.function"
      ],
      "settings": {
        "foreground": "#C7B0F0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.method",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#C7B0F0"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.class",
        "support.type",
        "storage.type.class",
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#8FCF98"
      }
    },
    {
      "scope": [
        "constant",
        "constant.other",
        "variable.other.constant",
        "support.constant"
      ],
      "settings": {
        "foreground": "#F9E2AF"
      }
    },
    {
      "scope": [
        "variable",
        "identifier",
        "variable.parameter",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#F6EDE6"
      }
    },
    {
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name",
        "entity.name.tag.xml",
        "punctuation.definition.tag.xml",
        "meta.structure.dictionary.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#99E1C9"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.attribute"
      ],
      "settings": {
        "foreground": "#99E1C9"
      }
    },
    {
      "scope": [
        "support.type.property-name.css",
        "support.constant.property-value.css",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#9FBCE8"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "punctuation",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#AFA9B8"
      }
    },
    {
      "scope": [
        "string.regexp",
        "constant.other.regex"
      ],
      "settings": {
        "foreground": "#99E1C9"
      }
    },
    {
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#C7B0F0"
      }
    }
  ],
  "semanticHighlighting": true,
  "publisher": "meowllow",
  "description": "A soft, mellow, and soothing color theme inspired by creamy pastels and cozy coding sessions."
}
