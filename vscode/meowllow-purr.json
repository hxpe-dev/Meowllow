{
  "name": "Meowllow Purr",
  "type": "dark",
  "colors": {
    "editor.background": "#2B2833",
    "editor.foreground": "#ECE7EE",
    "editorCursor.foreground": "#B4C5ED",
    "editorLineNumber.foreground": "#B7B0C0",
    "editorLineNumber.activeForeground": "#B4C5ED",
    "editor.selectionBackground": "#B4C5ED33",
    "editor.selectionHighlightBackground": "#B4C5ED1F",
    "editor.findMatchBackground": "#F8EAA338",
    "editor.findMatchHighlightBackground": "#B4C5ED29",
    "editor.wordHighlightBackground": "#8FCFA01F",
    "editor.wordHighlightStrongBackground": "#F2B8B21F",
    "editorBracketMatch.background": "#B4C5ED1F",
    "editorBracketMatch.border": "#B4C5ED",
    "editorGutter.modifiedBackground": "#8FCFA0",
    "editorGutter.addedBackground": "#8FCFA0",
    "editorGutter.deletedBackground": "#F26D6D",
    "activityBar.background": "#201E27",
    "activityBar.foreground": "#D9D4DE",
    "activityBarBadge.background": "#B4C5ED",
    "activityBarBadge.foreground": "#2B2833",
    "sideBar.background": "#24212B",
    "sideBar.foreground": "#D9D4DE",
    "statusBar.background": "#201E27",
    "titleBar.activeBackground": "#201E27",
    "titleBar.activeForeground": "#ECE7EE",
    "titleBar.inactiveBackground": "#24212B",
    "titleBar.inactiveForeground": "#B7B0C0",
    "tab.activeBackground": "#2B2833",
    "tab.inactiveBackground": "#24212B",
    "tab.activeForeground": "#ECE7EE",
    "tab.inactiveForeground": "#B7B0C0",
    "tab.border": "#24212B",
    "button.background": "#B4C5ED24",
    "button.foreground": "#ECE7EE",
    "badge.background": "#B4C5ED",
    "badge.foreground": "#2B2833",
    "input.background": "#4A4557",
    "input.foreground": "#ECE7EE",
    "editorError.foreground": "#F26D6D",
    "editorWarning.foreground": "#E6B85A",
    "editorInfo.foreground": "#6EA6F6",
    "editorHint.foreground": "#A9DDBB"
  },
  "semanticTokenColors": {
    "variable": "#ECE7EE",
    "variable.readonly": "#F8EAA3",
    "parameter": "#8EE8E4",
    "property": "#8EE8E4",
    "function": "#BFA8F6",
    "method": "#BFA8F6",
    "enumMember": "#F8EAA3",
    "class": "#8FCFA0",
    "type": "#8FCFA0",
    "interface": "#8FCFA0",
    "namespace": "#B4C5ED",
    "keyword": "#B4C5ED",
    "comment": {
      "foreground": "#7A7485",
      "fontStyle": "italic"
    },
    "string": "#F2B8B2",
    "number": "#F5C2E7",
    "regexp": "#8EE8E4",
    "operator": "#B7B0C0"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#7A7485",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#B4C5ED",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "keyword.control"
      ],
      "settings": {
        "foreground": "#B4C5ED",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "string",
        "constant.other.symbol",
        "string.quoted.template"
      ],
      "settings": {
        "foreground": "#F2B8B2"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.numeric.integer",
        "constant.numeric.float",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#F5C2E7"
      }
    },
    {
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call",
        "meta.function-call.identifier",
        "variable.function"
      ],
      "settings": {
        "foreground": "#BFA8F6",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.method",
        "meta.method-call"
      ],
      "settings": {
        "foreground": "#BFA8F6"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "support.class",
        "support.type",
        "storage.type.class",
        "entity.name.namespace"
      ],
      "settings": {
        "foreground": "#8FCFA0"
      }
    },
    {
      "scope": [
        "constant",
        "constant.other",
        "variable.other.constant",
        "support.constant"
      ],
      "settings": {
        "foreground": "#F8EAA3"
      }
    },
    {
      "scope": [
        "variable",
        "identifier",
        "variable.parameter",
        "variable.other.property"
      ],
      "settings": {
        "foreground": "#ECE7EE"
      }
    },
    {
      "scope": [
        "meta.object-literal.key",
        "support.type.property-name",
        "entity.name.tag.xml",
        "punctuation.definition.tag.xml",
        "meta.structure.dictionary.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#8EE8E4"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "meta.attribute"
      ],
      "settings": {
        "foreground": "#8EE8E4"
      }
    },
    {
      "scope": [
        "support.type.property-name.css",
        "support.constant.property-value.css",
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#B4C5ED"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "punctuation",
        "punctuation.definition.tag"
      ],
      "settings": {
        "foreground": "#B7B0C0"
      }
    },
    {
      "scope": [
        "string.regexp",
        "constant.other.regex"
      ],
      "settings": {
        "foreground": "#8EE8E4"
      }
    },
    {
      "scope": [
        "markup.bold",
        "markup.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#BFA8F6"
      }
    }
  ],
  "semanticHighlighting": true,
  "publisher": "meowllow",
  "description": "A soft, mellow, and soothing color theme inspired by creamy pastels and cozy coding sessions."
}
